{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "migration:generate": "npm run typeorm -- migration:create -c migration -n $1",
    "migrate": "npm run typeorm -- migration:run -c migration",
    "migrate:down": "npm run typeorm -- migration:revert -c migration",
    "seed:generate": "npm run typeorm -- migration:create -c seed -n $1",
    "seed": "npm run typeorm -- migration:run -c seed",
    "seed:down": "npm run typeorm -- migration:revert -c seed",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.3",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.3",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.3",
    "@nestjs/platform-socket.io": "^7.0.3",
    "@nestjs/swagger": "^4.4.0",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjs/websockets": "^7.0.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "express-session": "^1.17.0",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "sqlite3": "^4.1.1",
    "swagger-ui-express": "^4.1.3",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.3",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.2",
    "@types/passport-jwt": "^3.0.3",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.7.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
